<CancelarNfseEnvio xmlns="http://www.abrasf.org.br/ABRASF/arquivos/nfse.xsd">
  <Pedido xmlns="http://www.abrasf.org.br/ABRASF/arquivos/nfse.xsd">
    <InfPedidoCancelamento Id="{{pedido_cancelamento.id}}">
      <IdentificacaoNfse>
        <Numero> {{pedido_cancelamento.identificacao_nfse.numero}} </Numero>
        <Cnpj> {{pedido_cancelamento.identificacao_nfse.cnpj}} </Cnpj>
        {% if pedido_cancelamento.identificacao_nfse.inscricao_municipal %}
        <InscricaoMunicipal> {{pedido_cancelamento.identificacao_nfse.inscricao_municipal}} </InscricaoMunicipal>
        {% endif %}
        <CodigoMunicipio> {{pedido_cancelamento.identificacao_nfse.codigo_municipio}} </CodigoMunicipio>
      </IdentificacaoNfse>
      <CodigoCancelamento>0</CodigoCancelamento>
    </InfPedidoCancelamento>
  </Pedido>
</CancelarNfseEnvio>